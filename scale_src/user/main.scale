import std;
import foo.bar;

import spec.control;
import spec.numeric;
import spec.struct;
import spec.text;

fn swap(a:i32, b:i32) -> (i32, i32) {
    (b, bar.passthrough(a))
}

fn main() {
    std.print("Hello, world.\n\t\u{0482}\u{0f1c}\n");
    let a = 3;
    let b = 3;
    let i = 0;
    while i < 5 {
        (a, b) = swap(a-1, b+b);
        std.print("(" + a.to_string() + ", " + b.to_string() + ")\n");
        i = i + 1
    }
}

fn name_conflict() -> i32 {
    7
}

test "name conflict" {
    std.assert(name_conflict() != bar.name_conflict())
}

