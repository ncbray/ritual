extern fn abort();
extern fn print(value:string);

fn assert(value:bool) {
    if (!value) {
        print("Assert failed.\n");
        abort()
    }
}

fn assertEqualI32(a:i32, b:i32) {
    if (a != b) {
        print("Assert failed.\n");
        print("    ");
        print(a.to_string());
        print(" != ");
        print(b.to_string());
        print("\n");
        abort()
    }
}
